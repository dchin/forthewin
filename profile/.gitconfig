[gui]
[user]
  name = Rich Siegel
  email = rsiegel@ise.com
[push]
  default = tracking
[gui]
  encoding = utf-8
  recentrepo = C:/puppet/modules/puppet-winclusters
[alias]
  rich = !"git fetch upstream -v; git rebase upstream/master"
  st = status
  ci = commit
  br = branch
  co = checkout
  df = diff
  dc = diff --cached
  lg = log -p
  lol = log --graph --decorate --pretty=oneline --abbrev-commit
  lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
  ls = ls-files
[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan

[core]
	autocrlf = false
	editor = 'C:/Program Files (x86)/Notepad++/notepad++.exe' -multiInst -nosession -noPlugin
